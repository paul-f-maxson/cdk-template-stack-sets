{
  "name": "app",
  "version": "0.1.0",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "bin": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "tag-staging": "git tag --annotate --force --message 'move to $(git rev-parse --short HEAD)' app-staging",
    "stage": "yarn run tag-staging && git push --follow-tags staging master"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "18.16.2",
    "aws-cdk": "2.77.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.77.0",
    "constructs": "^10.2.9",
    "source-map-support": "^0.5.21"
  }
}
